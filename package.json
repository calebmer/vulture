{
  "name": "vulture",
  "version": "3.6.2",
  "description": "A light front-end framework based around the fast virtual DOM.",
  "author": "Caleb Meredith <calebmeredith8@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/calebmer/vulture#readme",
  "repository": "calebmer/vulture",
  "keywords": [
    "vulture",
    "virtual",
    "dom",
    "virtual-dom",
    "framework",
    "fast",
    "light",
    "front",
    "end",
    "frontend"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "dom.js",
    "string.js",
    "extras.js",
    "component.js",
    "jsx.js",
    "lib"
  ],
  "scripts": {
    "test": "$(npm bin)/mocha test/*.test.js",
    "lint": "$(npm bin)/eslint *.js lib/**/*.js test/**/*.js",
    "build": "npm run build-max && npm run build-min",
    "build-max": "MINIFY=false $(npm bin)/webpack",
    "build-min": "MINIFY=true $(npm bin)/webpack",
    "preversion": "git checkout master && npm run lint && npm test",
    "version": "npm run build && git add dist",
    "postversion": "git push origin master --tags && npm publish"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "vdom-thunk": "^3.0.0",
    "vdom-to-html": "^2.2.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "jsdom": "^7.2.2",
    "mocha": "^2.3.4",
    "standard": "^5.4.1",
    "webpack": "^1.12.9"
  }
}
